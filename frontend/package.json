{
  "name": "aarambh-ai-frontend",
  "version": "1.0.0",
  "description": "AI-Powered Educational Platform Frontend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --run --config vitest.config.unit.ts",
    "test:integration": "vitest --run --config vitest.config.integration.ts",
    "test:e2e": "playwright test",
    "test:components": "vitest --run src/tests/components",
    "test:smoke": "vitest --run src/tests/smoke",
    "test:performance": "vitest --run src/tests/performance",
    "test:security": "vitest --run src/tests/security",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html"
  },
  "dependencies": {
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^6.3.0",
    "@mui/material": "^6.3.0",
    "@mui/x-charts": "^7.22.2",
    "@mui/x-data-grid": "^7.22.2",
    "@tanstack/react-query": "^5.83.0",
    "@types/three": "^0.171.0",
    "axios": "^1.7.9",
    "firebase": "^11.10.0",
    "framer-motion": "^12.1.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "recharts": "^2.15.0",
    "socket.io-client": "^4.8.1",
    "three": "^0.171.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.2.0",
    "@vitest/ui": "^2.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "happy-dom": "^15.11.6",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vite": "^6.0.5",
    "vite-bundle-analyzer": "^0.11.0",
    "vitest": "^2.2.0"
  }
}